{
  "name": "graphql",
  "version": "1.0.1",
  "groupId": "com.xlongwei",
  "artifactId": "graphql",
  "schemaPackage": "com.xlongwei.graphql",
  "schemaClass": "GraphqlSchema",
  "overwriteSchemaClass": true,
  "httpPort": 8083,
  "enableHttp": true,
  "httpsPort": 8443,
  "enableHttps": false,
  "enableHttp2": false,
  "enableRegistry": false,
  "supportDb": false,
  "dbInfo": {
    "name": "mysql",
    "driverClassName": "com.mysql.jdbc.Driver",
    "jdbcUrl": "jdbc:mysql://mysqldb:3306/oauth2?useSSL=false",
    "username": "root",
    "password": "my-secret-pw"
  },
  "supportH2ForTest": false,
  "supportClient": false
}
