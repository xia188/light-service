{
  "rootPackage": "com.xlongwei.petstore",
  "handlerPackage": "com.xlongwei.petstore.handler",
  "modelPackage": "com.xlongwei.petstore.model",
  "artifactId": "petstore",
  "groupId": "com.xlongwei",
  "name": "petstore",
  "version": "3.0.1",
  "overwriteHandler": true,
  "overwriteHandlerTest": true,
  "httpPort": 8083,
